            <variable name="dbClusterContainer"/>
            <viewVariable name="dbClusterContainerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:dbClusterContainer">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="observability_emf.DatabaseCluster" referenceName="clusters"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="modelContainer" containerMappings="//@ownedViewpoints[name='obsView']/@ownedRepresentations[name='obsDiag']/@defaultLayer/@containerMappings[name='model']">
