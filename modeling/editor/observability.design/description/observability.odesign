                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="['metric'+baseMetricContainer.collectedMetrics->size()/]"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
