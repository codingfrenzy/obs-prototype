          <ownedTools xsi:type="tool:NodeCreationDescription" name="baseMetricNode" nodeMappings="//@ownedViewpoints[name='obsView']/@ownedRepresentations[name='obsDiag']/@defaultLayer/@nodeMappings[name='baseMetric']">
