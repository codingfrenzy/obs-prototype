          </style>
        </edgeMappings>
        <containerMappings name="model" semanticCandidatesExpression="feature:hasModel" domainClass="observability_emf.Model" reusedContainerMappings="//@ownedViewpoints[name='obsView']/@ownedRepresentations[name='obsDiag']/@defaultLayer/@containerMappings[name='model']/@subContainerMappings[name='dbCluster']">
          <subContainerMappings name="dbCluster" domainClass="observability_emf.DatabaseCluster" reusedNodeMappings="//@ownedViewpoints[name='obsView']/@ownedRepresentations[name='obsDiag']/@defaultLayer/@containerMappings[name='model']/@subContainerMappings[name='dbCluster']/@subNodeMappings[name='nodeMachine']">
            <subNodeMappings name="nodeMachine" semanticCandidatesExpression="feature:machines" domainClass="observability_emf.NodeMachine">
              <style xsi:type="style:SquareDescription" labelExpression="machine" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
              </style>
            </subNodeMappings>
