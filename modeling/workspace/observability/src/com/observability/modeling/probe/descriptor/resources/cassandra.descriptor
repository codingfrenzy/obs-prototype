<Plugin "java">

	JVMARG "-Djava.class.path=/opt/collectd/java-dep/generic-jmx.jar:/opt/collectd/java-dep/collectd-api.jar"
	LoadPlugin "org.collectd.java.GenericJMX"
<Plugin "GenericJMX">

   # Size of the data on the node.
   <MBean "data-load"> @MetricDB
  	 ObjectName "org.apache.cassandra.metrics:type=Storage,name=Load" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "data-load" @Attribute
  	   Attribute "Count" @Attribute
  	 </Value>
   </MBean>

   # Number of compaction operations queued and waiting to run.
   <MBean "pending-compaction-tasks"> @MetricDB
  	 ObjectName "org.apache.cassandra.db:type=CompactionManager" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "pending-compaction-tasks" @Attribute
  	   Attribute "PendingTasks" @Attribute
  	 </Value>
   </MBean>

   # Number of memtables queued for the flush process.
   <MBean "pending-flushes"> @MetricDB
  	 ObjectName "org.apache.cassandra.internal:type=FlushWriter" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "pending-flushes" @Attribute
  	   Attribute "PendingTasks" @Attribute
  	 </Value>
   </MBean>

   # Number of gossip messages and acknowledgments queued and waiting to be sent
   # or received.
   <MBean "pending-gossip-tasks"> @MetricDB
  	 ObjectName "org.apache.cassandra.internal:type=GossipStage" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "pending-gossip-tasks" @Attribute
  	   Attribute "PendingTasks" @Attribute
  	 </Value>
   </MBean>

   # Number of hints in the queue waiting to be delivered after a failed node
   # comes up.
   <MBean "pending-hinted-handoff"> @MetricDB
  	 ObjectName "org.apache.cassandra.internal:type=HintedHandoff" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "pending-hinted-handoff" @Attribute
  	   Attribute "PendingTasks" @Attribute
  	 </Value>
   </MBean>

   # Number of read requests received by the cluster and waiting to be handled.
   <MBean "pending-read-ops"> @MetricDB
  	 ObjectName "org.apache.cassandra.request:type=ReadStage" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "pending-read-ops" @Attribute
  	   Attribute "PendingTasks" @Attribute
  	 </Value>
   </MBean>

   # Number of read repair operations in the queue waiting to run.
   <MBean "pending-read-repair-tasks"> @MetricDB
  	 ObjectName "org.apache.cassandra.request:type=ReadRepairStage" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "pending-read-repair-tasks" @Attribute
  	   Attribute "PendingTasks" @Attribute
  	 </Value>
   </MBean>

   # Manual repair tasks pending, operations to be completed during anti-entropy
   # repair of a node.
   <MBean "pending-repair-tasks"> @MetricDB
  	 ObjectName "org.apache.cassandra.internal:type=AntiEntropyStage" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "pending-repair-tasks" @Attribute
  	   Attribute "PendingTasks" @Attribute
  	 </Value>
   </MBean>

   # Pending tasks related replication of data after an insert or update to a
   # row.
   <MBean "pending-repl-on-write-tasks"> @MetricDB
  	 ObjectName "org.apache.cassandra.request:type=ReplicateOnWriteStage" @Attribute
  	 <Value> @Attribute
  	   Type "gauge" @Attribute
  	   InstancePrefix "pending-repl-on-write-tasks" @Attribute
  	   Attribute "PendingTasks" @Attribute
  	 </Value>
   </MBean>

   # Progress of streamed rows from the receiving node.
   <MBean "pending-request-responses">
  	 ObjectName "org.apache.cassandra.request:type=RequestResponseStage"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "pending-request-responses"
  	   Attribute "PendingTasks"
  	 </Value>
   </MBean>

   # Number of write requests received by the cluster and waiting to be handled.
   <MBean "pending-write-ops">
  	 ObjectName "org.apache.cassandra.request:type=MutationStage"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "pending-write-ops"
  	   Attribute "PendingTasks"
  	 </Value>
   </MBean>

   # Average response time to a client read request.
   <MBean "read-latency-op">
  	 ObjectName "org.apache.cassandra.metrics:type=ClientRequest,scope=Read,name=Latency"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "read-latency-op"
  	   Attribute "Mean"
  	 </Value>
   </MBean>

   # The number of read requests.
   <MBean "read-ops">
  	 ObjectName "org.apache.cassandra.metrics:type=ClientRequest,scope=Read,name=Latency"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "read-ops"
  	   Attribute "Count"
  	 </Value>
   </MBean>

   # The average response time to a client write request.
   <MBean "write-latency-op">
  	 ObjectName "org.apache.cassandra.metrics:type=ClientRequest,scope=Write,name=Latency"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "write-latency-op"
  	   Attribute "Mean"
  	 </Value>
   </MBean>

   # The write requests.
   <MBean "write-ops">
  	 ObjectName "org.apache.cassandra.metrics:type=ClientRequest,scope=Write,name=Latency"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "write-ops"
  	   Attribute "Count"
  	 </Value>
   </MBean>

   # Cache requests that resulted in a key cache hit.
   <MBean "cf-keycache-hit-rate">
  	 ObjectName "org.apache.cassandra.metrics:type=Cache,scope=KeyCache,name=HitRate"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-keycache-hit-rate"
  	   Attribute "Value"
  	 </Value>
   </MBean>

   # Number of read requests that resulted in the requested row key being found
   # in the key cache.
   <MBean "cf-keycache-hits">
  	 ObjectName "org.apache.cassandra.metrics:type=Cache,scope=KeyCache,name=Hits"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-keycache-hits"
  	   Attribute "Count"
  	 </Value>
   </MBean>

   # Total number of read requests on the key cache.
   <MBean "cf-keycache-requests">
  	 ObjectName "org.apache.cassandra.metrics:type=Cache,scope=KeyCache,name=Requests"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-keycache-requests"
  	   Attribute "Count"
  	 </Value>
   </MBean>

   # Disk space used by a column family for readable data.
   <MBean "cf-live-disk-used">
  	 ObjectName "org.apache.cassandra.metrics:type=ColumnFamily,name=LiveDiskSpaceUsed"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-live-disk-used"
  	   Attribute "Value"
  	 </Value>
   </MBean>

   # Number of pending reads and writes on a column family.
   <MBean "cf-pending-tasks-system-schema_keyspaces">
  	 ObjectName "org.apache.cassandra.metrics:type=ColumnFamily,keyspace=system,scope=schema_keyspaces,name=PendingTasks"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-pending-tasks-system-schema_keyspaces"
  	   Attribute "Value"
  	 </Value>
   </MBean>

   # Internal response time to a successful request to read data from a column
   # family.
   <MBean "cf-read-latency-op">
  	 ObjectName "org.apache.cassandra.metrics:type=ColumnFamily,name=ReadLatency"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-read-latency-op"
  	   Attribute "Mean"
  	 </Value>
   </MBean>

   # Read requests per second on a column family.
   <MBean "cf-read-ops">
  	 ObjectName "org.apache.cassandra.metrics:type=ColumnFamily,name=ReadLatency"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-read-ops"
  	   Attribute "MeanRate"
  	 </Value>
   </MBean>

   # Cache requests that resulted in a row cache hit.
   <MBean "cf-rowcache-hit-rate">
  	 ObjectName "org.apache.cassandra.metrics:type=Cache,scope=RowCache,name=HitRate"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-rowcache-hit-rate"
  	   Attribute "Value"
  	 </Value>
   </MBean>

   # Number of read requests on the row cache.
   <MBean "cf-rowcache-hits">
  	 ObjectName "org.apache.cassandra.metrics:type=Cache,scope=RowCache,name=Hits"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-rowcache-hits"
  	   Attribute "Count"
  	 </Value>
   </MBean>

   # Total number of read requests on the row cache.
   <MBean "cf-rowcache-requests">
  	 ObjectName "org.apache.cassandra.metrics:type=Cache,scope=RowCache,name=Requests"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-rowcache-requests"
  	   Attribute "Count"
  	 </Value>
   </MBean>

   # Disk space used by a column family for live or old data (not live).
   <MBean "cf-total-disk-used">
  	 ObjectName "org.apache.cassandra.metrics:type=ColumnFamily,name=TotalDiskSpaceUsed"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-total-disk-used"
  	   Attribute "Value"
  	 </Value>
   </MBean>

   # Internal response time to a successful request to write data to a column
   # family.
   <MBean "cf-write-latency-op">
  	 ObjectName "org.apache.cassandra.metrics:type=ColumnFamily,name=WriteLatency"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-write-latency-op"
  	   Attribute "Mean"
  	 </Value>
   </MBean>

   # Write requests per second on a column family.
   <MBean "cf-write-ops">
  	 ObjectName "org.apache.cassandra.metrics:type=ColumnFamily,name=WriteLatency"
  	 <Value>
  	   Type "gauge"
  	   InstancePrefix "cf-write-ops"
  	   Attribute "MeanRate"
  	 </Value>
   </MBean>

   # Allocated, init, max and used memory guaranteed for the Java heap.
   <MBean "heap-mem">
  	 ObjectName "java.lang:type=Memory"
  	 <Value>
  	   Type "memory"
  	   InstancePrefix "heap-mem"
  	   Table true
  	   Attribute "HeapMemoryUsage"
  	 </Value>
   </MBean>

   # Allocated, init, max and used memory guaranteed for the Java nonheap.
   <MBean "nonheap-mem">
  	 ObjectName "java.lang:type=Memory"
  	 <Value>
  	   Type "memory"
  	   InstancePrefix "nonheap-mem"
  	   Table true
  	   Attribute "NonHeapMemoryUsage"
  	 </Value>
   </MBean>  


	<Aggregation> @MetricAggregated(memory)
		Host "/128.+|45.+/" @Attribute
		Plugin "GenericJMX" @Attribute
		Type "memory" @Attribute
		#TypeInstance "/^heap-mem.*/"
		GroupBy "TypeInstance" @Attribute

		# Sets the name of the output folder
		SetHost "global" @Attribute
		SetPlugin "aggregation" @Attribute
		SetPluginInstance "heap-mem-%{aggregation}" @Attribute

		CalculateNum true  
		CalculateSum true 
		CalculateAverage false 
		CalculateMinimum false 
		CalculateMaximum false 
		CalculateStddev false 
	</Aggregation>


