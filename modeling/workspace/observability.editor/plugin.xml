<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated observability -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.observability.modeling.emf.presentation.EmfModelWizardID"
            name="%_UI_EmfModelWizard_label"
            class="com.observability.modeling.emf.presentation.EmfModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/EmfModelFile.gif">
         <description>%_UI_EmfModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated observability -->
      <editor
            id="com.observability.modeling.emf.presentation.EmfEditorID"
            name="%_UI_EmfEditor_label"
            icon="icons/full/obj16/EmfModelFile.gif"
            extensions="emf"
            class="com.observability.modeling.emf.presentation.EmfEditor"
            contributorClass="com.observability.modeling.emf.presentation.EmfActionBarContributor">
      </editor>
   </extension>

</plugin>
